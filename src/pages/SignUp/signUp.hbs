<main class="main-sign">
    <form class="sign-up-form form" action="">
        <h2 class="sign-up-form__header form-header">Sign up</h2>
        <div class="sign-up-form__wrap input-wrap">
            {{{inputPhone}}}
            {{{inputEmail}}}
            {{{inputLogin}}}
            {{{inputFirstname}}}
            {{{inputLastname}}}
            {{{inputPassword}}}
            {{{inputPassword2}}}
        </div>
        <div class="sign-up-form__actions form-actions">
            {{{formButton}}}
            <span class="sign-up-form__sign-in-wrap under-submit">
                {{{loginLink}}}
            </span>
        </div>
    </form>
</main>
